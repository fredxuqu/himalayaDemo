<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="NewsDao">
	
	<select id="list" resultType="News">
		SELECT 	id,
				title,
				content,
				author,
				type,
				publish_time
		  FROM  tb_news
	</select>
</mapper>
